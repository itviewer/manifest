<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- 注意不是 git://github.com -->
  <remote fetch="git@github.com:itviewer" name="itviewer"/>
  <remote fetch="git@github.com" name="github"/>

  <default revision="main" remote="itviewer" sync-j="4" />
  <project path="repo1" name="repo1" />
  <!-- 重命名克隆后的目录名称，且指定 revision -->
  <project path="test2" name="repo2" revision="dev" />
  <!-- 重新组织本地路径，且指定 remote 和路径 -->
  <project path="test3/repo3" name="apieasy/repo3" remote="github" />
</manifest>
